exports.createToken = (req, res) => {

    var jwt = require('jsonwebtoken');
    var token = jwt.sign(
        {
            mail: req.body.mail,
            name: req.body.name
        }, 
        process.env.TOKEN_SECRET || "[z43s=_9u3^Er;m",
        {
            expiresIn: "2h"
        }
    )
    res.status(200).json({
        msg: "msssg",
        token: token
    })
}